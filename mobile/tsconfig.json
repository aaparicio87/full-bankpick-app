{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@components/*": ["./src/components/*"],
      "@contexts/*": ["./src/contexts/*"],
      "@state/*": ["./src/state/*"],
      "@features/*": ["./src/features/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@styles/*": ["./src/styles/*"],
      "@storage": ["./src/storage/index"],
      "@utils/*": ["./src/utils/*"],
      "@/types/*": ["./src/types/*"],
    },
  },
}
